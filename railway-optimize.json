{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "apps/workers/optimize/Dockerfile"
  },
  "deploy": {
    "startCommand": "node apps/workers/optimize/index.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "sleepApplication": false,
    "numReplicas": 1
  }
}