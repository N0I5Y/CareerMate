# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Queue Configuration
QUEUE_TEMPLATE=resume.template
QUEUE_EXTRACT=resume.extract
QUEUE_OPTIMIZE=resume.optimize
QUEUE_CONVERT=resume.convert

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Storage Configuration
USE_S3=false
LOCAL_DATA_DIR=./data

# Logging
LOG_LEVEL=info

# Link Extraction
EXTRACT_LINKS_APPEND=1
EXTRACT_LINKS_MAX=20

# Prompt Management
ALLOW_PROMPT_WRITE=true
PROMPT_WRITE_TOKEN=your_admin_token_here

# External Services (Optional)
TWILIO_SID=your_twilio_sid
TWILIO_TOKEN=your_twilio_token
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key